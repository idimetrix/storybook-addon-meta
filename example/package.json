{
	"name": "storybook-addon-meta-example",
	"version": "1.0.1",
	"private": true,
	"scripts": {
		"nuxt:dev": "nuxt",
		"nuxt:build": "nuxt build",
		"nuxt:start": "nuxt start",
		"nuxt:generate": "nuxt generate",
		"storybook:serve": "start-storybook -p 6006",
		"storybook:build": "build-storybook",
		"storybook:build:serve": "cd storybook-static && ws --spa index.html",
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"build:serve": "cd dist && ws --spa index.html",
		"proxy": "lt --port 8080 --subdomain same",
		"test:unit": "vue-cli-service test:unit",
		"test:unit:watch": "vue-cli-service test:unit --watch",
		"test:unit:snapshot": "vue-cli-service test:unit -u",
		"test:e2e": "vue-cli-service test:e2e",
		"lint:style": "stylelint '(src|tests)/**/*.(css|scss|sass|vue)' '*.(css|scss|sass|vue)'",
		"lint:style:fix": "npm run lint:style -- --fix",
		"lint:code": "vue-cli-service lint {src,tests}/**/*.{ts,tsx,vue} *.{js,jsx,ts,tsx,vue}",
		"lint:code:fix": "npm run lint:code -- --fix",
		"lint:all": "npm run lint:style && npm run lint:code",
		"lint:all:fix": "npm run lint:style:fix && npm run lint:code:fix",
		"commit": "git add -A && git commit -m 'deploy' --no-verify && git push origin HEAD",
		"analyze": "source-map-explorer 'dist/**/*.js'",
		"prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,vue,htm,html,json,xml,css,scss,sass}' '*.{js,jsx,ts,tsx,vue,htm,html,json,xml,css,scss,sass}'",
		"format": "npm run prettier && npm run lint:all:fix",
		"assets:svg": "vsvg -s ./static/svg -t ./src/components/svg --ext ts",
		"ncu": "ncu -u --timeout 100000  && npm i  && npm run format"
	},
